graph PlayerSFX {
    func PlaySFXOnce(SFXType SoundEffects){
        PlaySound(thisEntity<Player>, SFXType)
    }

    async func PlaySFXOverTime(SFXType SoundEffects, millisecond int){
        PlaySound(thisEntity<Player>, SFXType) 
        // Play(createdSFX, true)
        WaitForSeconds(millisecond)
    }
}